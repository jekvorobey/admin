{
    "name": "webpack-blank-project",
    "version": "1.0.0",
    "private": true,
    "repository": "https://gitlab.com/greensight/webpack-blank-project/",
    "license": "MIT",
    "browserslist": {
        "production": [
            "> 0.25%",
            "ie 11",
            "ios 9.3",
            "not op_mini all",
            "not android <= 4.4",
            "not chrome <= 49"
        ],
        "development": [
            "last 2 chrome versions"
        ]
    },
    "scripts": {
        "dev": "cross-env BROWSERSLIST_ENV=development webpack-dev-server --env.mode dev",
        "dev-build": "cross-env BROWSERSLIST_ENV=development webpack --env.mode dev",
        "prod": "cross-env BROWSERSLIST_ENV=production webpack --env.mode prod",
        "analyze": "yarn prod --env.presets analyze",
        "buddy": "yarn prod --env.presets buddy",
        "stats": "yarn prod --profile --progress --json > stats.json",
        "static": "static public -p 8080 -a 0.0.0.0",
        "eslint": "eslint '**/*.js' --fix",
        "stylelint": "stylelint **/*.css --fix",
        "clear-cache": "rm -rf node_modules/.cache",
        "rebuild": "yarn clear-cache && rm -rf public && yarn prod",
        "postinstall": "yarn clear-cache",
        "webpack-defaults": "webpack-defaults"
    },
    "lint-staged": {
        "*.js": [
            "yarn eslint",
            "git add"
        ],
        "*.css": [
            "yarn stylelint",
            "git add"
        ]
    },
    "eslintIgnore": [
        "node_modules",
        "build",
        "!.*.js"
    ],
    "devDependencies": {
