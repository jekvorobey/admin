{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:maxim.kyprov/laravel-health-check.git"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-common-ms-code.git"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-oms-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-pim-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-merchant-management-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-store-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-message-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-marketing-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-cm-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-cms-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-logistics-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-catalog-import-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-reports-lib"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "barryvdh/laravel-debugbar": "^3.6",
        "box/spout": "^3.3",
        "ibt/catalog-import-lib": "dev-master",
        "ibt/cm-lib": "dev-master",
        "ibt/cms-lib": "dev-master",
        "ibt/common-lib": "dev-master",
        "ibt/logistics-lib": "dev-master",
        "ibt/marketing-lib": "dev-master",
        "ibt/merchant-management-lib": "dev-master",
        "ibt/message-lib": "dev-feature/BX-6594-custom-filter-chats as dev-master",
        "ibt/oms-lib": "dev-master",
        "ibt/pim-lib": "dev-master",
        "ibt/reports-lib": "dev-master",
        "ibt/store-lib": "dev-master",
        "laravel/framework": "^9.0",
        "laravel/tinker": "^2.7",
        "letsgoi/laravel-health-check": "dev-master",
        "phpoffice/phpspreadsheet": "^1.23",
        "sentry/sentry-laravel": "^2.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/laravel-dump-server": "^1.8",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^8.1",
        "spatie/ray": "^1.34",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "phpunit": "./vendor/bin/phpunit",
        "phpcs": "./vendor/bin/phpcs --standard=phpcs.xml",
        "phpcbf": "./vendor/bin/phpcbf --standard=phpcs.xml"
    }
}
